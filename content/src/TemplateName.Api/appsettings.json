{
  "ConnectionStrings": {
    "Default": "Username=postgres;Host=localhost;Port=5432;Database=postgres;Pooling=true;"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Error"
      }
    },
    "Enrich": [ "FromLogContext" ],
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            { "Name": "Console" },
            {
              "Name": "Seq",
              "Args": { "serverUrl": "http://localhost:5341" }
            }
          ]
        }
      }
    ]
  }
}
